{
  "expo": {
    "name": "Qur8",
    "slug": "Qur8",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["assets/*"],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Grant Qur8 permission to access your photo gallery for a richer experience!"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
